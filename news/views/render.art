{{extend './common/layout.art'}}
{{block 'main'}}
    <div class="content">
        <div class="box1 btn1">
            <h4>文章的标题为：</h4><li>{{searchs && searchs.title}}</li>
        </div>
        <div class="box1 btn2">
            <h4>文章的类别为：</h4><li>{{searchs && searchs.channelName}}</li>
        </div>
        <div class="box1 btn3">
            <h4>文章的内容为：</h4><li>{{searchs && searchs.content}}</li>
        </div>
    </div>
{{/block}}